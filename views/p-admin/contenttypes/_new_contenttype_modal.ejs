<div class="ts-modal-hidden-container">
  <div id="new-contenttype-modal" data-name="new-contenttype-modal" class="ts-modal ts-modal-effect-1">
    <section class="ts-modal-content ts-bg-text-primary-color  ts-no-heading-margin ts-padding-md ts-shadow ">
      <form class="ts-form async-admin-ajax-forms" name="new-contenttype-form" id="new-contenttype-form" method="post" action="/<%- locals.adminPath %>/content/contenttype/new" data-successsubmitfunction="adminRefresh">
        <input type="hidden" name="_csrf" value=<%- locals.token %> >
        <div  id="new-contenttype-modal-content">
          <div class="ts-row ts-form-row">
            <div>
              <label class="ts-label">Content Type Title</label>
            </div>
            <input type="text" placeholder="title" class="ts-input" name="title">
          </div>
        </div>
        <div class="ts-row ts-form-row">
          <div class="ts-col-span12 ts-text-center">
            <input type="submit" name="new_contenttype_button" value="create content type" class="ts-button ts-button-primary-color ts-modal-close"/>
            <a class="ts-button ts-modal-close">cancel</a>
          </div>
        </div>
      </form>
    </section>
  </div>
  <div class="ts-modal-overlay ts-bg-primary-color"></div>
</div>