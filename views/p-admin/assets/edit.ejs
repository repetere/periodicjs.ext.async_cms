<% genericdoc = asset %>
<% genericdocid = asset._id %>
<% genericdoctype = "Asset" %>
<% genericdoctypename = "asset" %>
<% genericdoctypelink = "asset" %>
<% genericdoctypeplurallink = "assets" %>
<% include ../../shared/header %>
<div class="ts-container">
  <div class="ts-text-container ts-genericdoc">
    <% include ../../shared/genericdoc_compose_title %>

    <div class="ts-row">
      <div class="ts-col-span12">
        <div id="refresh-tabs" class="ts-tabs">
          <nav>
            <ul class="ts-col-span12 ts-text-center">
              <li class="ts-col-span3 ts-no-margin">Basic</li>
              <li class="ts-col-span3 ts-no-margin">Advanced</li>
            </ul>
          </nav>
          <div class="ts-tabs-content">
            <section class="ts-no-padding">
              <form id="edit-<%- genericdoctypename %>-wysiwyg-form" name="edit-<%- genericdoctypename %>-form" class="ts-form async-admin-ajax-forms" method="post" action="/<%- locals.adminPath %>/content/<%- genericdoctypename %>/<%- genericdoc._id %>/edit" enctype="multipart/form-data" 
          data-successsubmitfunction="adminRefresh">
                <% include compose_asset_form %>
              </form>
            </section>
            <section class="ts-no-padding">
              <% include ../../shared/genericdoc_show_doc %>
            </section>
          </div>
        </div>
      </div>
    </div>

    
  </div>
</div>
<script type="text/javascript" src="/extensions/periodicjs.ext.async_cms/js/cms.min.js"></script>
<% include ../../shared/footer %>