<div class="ts-modal-hidden-container">
  <div id="new-asset-modal" data-name="new-asset-modal" class="ts-modal ts-modal-effect-1">
    <section class="ts-modal-content ts-bg-text-primary-color  ts-no-heading-margin ts-padding-md ts-shadow ">
      <form class="ts-form async-admin-ajax-forms" name="new-asset-form" id="new-asset-form" method="post" action="/<%- locals.adminPath %>/content/asset/new" data-successsubmitfunction="adminRefresh" enctype="multipart/form-data" >
        <input type="hidden" name="_csrf" value=<%- locals.token %> >
        <div  id="new-asset-modal-content">
          <div class="ts-row ts-form-row">
            <div>
              <label class="ts-label">Upload new files</label>
            </div>
            <input type="file" 
              value="Upload new assets"
              id="padmin-assets"
              multiple="multiple" 
              class="ts-col-span12 autoFormSubmit ts-modal-close" 
              placeholder="Upload Assets" 
              name="mediafiles" 
              title="upload-assets" 
              />
          </div>
        </div>
        <div class="ts-row ts-form-row">
          <div class="ts-col-span12 ts-text-center">
            <a class="ts-button ts-modal-close">cancel</a>
          </div>
        </div>
      </form>
    </section>
  </div>
  <div class="ts-modal-overlay ts-bg-primary-color"></div>
</div>