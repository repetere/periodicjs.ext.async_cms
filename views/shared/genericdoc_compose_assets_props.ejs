<% if(genericdoc){%>
	<details class="ts-text-sm">
		<summary>Asset Options</summary>
		<aside>
			<div class="ts-row ts-form-row">
				<div class="ts-col-span12">
				  <label class="ts-label"  for="docfileurllabel">file url:</label>
				</div>
				<div class="ts-col-span12">
				  <input type="text" id="docfileurllabel" class="ts-input ts-text-sm" value="<%- genericdoc.fileurl %>" >
				</div>
			</div>
			<div class="ts-row ts-form-row">
				<div class="ts-col-span12">
				  <label  class="ts-label" id="doclocationtypelabel">location type:</label>
				</div>
				<div class="ts-col-span12">
					<input type="text" id="doclocationtypelabel" class="ts-input ts-text-sm" name="name" value="<%- genericdoc.locationtype %>" >
				</div>
			</div>
			<% if(genericdoc.attributes && genericdoc.attributes.periodicFilename){%>
			<div class="ts-row ts-form-row">
				<div class="ts-col-span12">
				  <label  class="ts-label" id="docperiodicfilenamelabel">Periodic Filename:</label>
				</div>
				<div class="ts-col-span12">
					<input type="text" id="docperiodicfilenamelabel" class="ts-input ts-text-sm" name="name" value="<%- genericdoc.attributes.periodicFilename %>" >
				</div>
			</div>
			<%}%>
		</aside>
	</details>
<%}%>
